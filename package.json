{
  "name": "smartdealsiq",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "expo:dev": "npx expo start --localhost",
    "server:dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=cjs --outdir=server_dist",
    "server:prod": "node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "electron:dev": "concurrently \"npm run expo:dev\" \"wait-on http://localhost:8081 && electron electron/main.js\"",
    "electron:build": "npm run expo:static:build && electron-builder",
    "electron:build:win": "npm run expo:static:build && electron-builder --win",
    "electron:build:mac": "npm run expo:static:build && electron-builder --mac",
    "electron:build:linux": "npm run expo:static:build && electron-builder --linux"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@expo/vector-icons": "^15.0.2",
    "@google/genai": "^1.34.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@tanstack/react-query": "^5.90.7",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.1",
    "expo": "~54.0.31",
    "expo-audio": "^1.1.1",
    "expo-blur": "^15.0.7",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.6",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "^17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "^19.0.8",
    "expo-notifications": "^0.32.16",
    "expo-sensors": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.15.0",
    "p-limit": "^7.2.0",
    "p-retry": "^7.1.1",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "stripe": "^20.1.0",
    "tsx": "^4.20.6",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.5.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.10.0",
    "@types/pg": "^8.16.0",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.4",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-updater": "^6.1.7",
    "esbuild": "^0.27.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.2",
    "wait-on": "^7.2.0"
  },
  "private": true
}
